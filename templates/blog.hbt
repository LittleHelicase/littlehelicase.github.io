{{> header}}

<div class="content">
<h2>{{title}}</h2>

    <ul>
        {{#each collections.posts}}
            <li>
                <h3><a href="{{link this.path}}">{{this.title}}</a></h3>
                <article>{{{ this.excerpt }}}</article>
            </li>
        {{/each}}
    </ul>
</div>

{{> footer}}
